for (let l = 0; l < 1; l++) {
    led.plotBrightness(2, 4, 100)
    basic.pause(200)
    led.plotBrightness(2, 3, 150)
    basic.pause(200)
    led.plotBrightness(2, 2, 200)
    basic.pause(200)
    led.plotBrightness(2, 4, 0)
    basic.pause(200)
    led.plotBrightness(2, 3, 0)
    basic.pause(200)
    led.plotBrightness(1, 1, 100)
    led.plotBrightness(3, 1, 100)

}
